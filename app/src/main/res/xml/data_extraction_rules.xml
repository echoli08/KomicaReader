<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <!-- 繁體中文註解：雲端備份納入我的最愛，避免重裝遺失 -->
    <cloud-backup>
        <include domain="sharedpref" path="KomicaFavorites.xml" />
    </cloud-backup>

    <!-- 繁體中文註解：裝置轉移納入我的最愛，避免換機遺失 -->
    <device-transfer>
        <include domain="sharedpref" path="KomicaFavorites.xml" />
    </device-transfer>
</data-extraction-rules>
